{
	"name": "fieldforce_dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "npx rimraf ./.svelte-kit;npx rimraf ./node_modules/.vite",
		"build": "npx rimraf public && npm run build:functions && vite build",
		"build:functions": "cd functions && npm run build && cd ..",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch --force ",
		"deploy": "firebase deploy --only hosting,database,firestore,storage,functions",
		"deploy:functions": "firebase deploy --only functions",
		"deploy:hostings": "firebase deploy --only hosting",
		"dev": "npm run setup:local && npm run clean && vite dev --port 5050",
		"dev:debug": "npm run setup:local && vite dev --port 5050 --debug",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"setup:development": "node flavours/development/setup.js",
		"setup:local": "node flavours/local/setup.js",
		"setup:production": "node flavours/production/setup.js",
		"test": "npm run test:integration && npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@cloudparker/easy-cropperjs-svelte": "^2.4.0",
		"@cloudparker/easy-idb": "^1.2.0",
		"@cloudparker/easy-script-loader-svelte": "^4.1.1",
		"@cloudparker/moldex.js": "^0.0.95",
		"@playwright/test": "^1.51.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.19.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.0.13",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.1.0",
		"firebase": "^11.4.0",
		"globals": "^16.0.0",
		"moment": "^2.30.1",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"shelljs": "^0.9.1",
		"svelte": "^5.23.0",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.1",
		"vitest": "^3.0.8"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.8.3",
		"nanoid": "^5.1.3"
	}
}
